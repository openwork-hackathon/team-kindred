{
  "framework": "nextjs",
  "buildCommand": "next build",
  "installCommand": "pnpm install",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "Vercel-CDN-Cache-Control", "value": "no-store, max-age=0" },
        { "key": "CDN-Cache-Control", "value": "no-store, max-age=0" }
      ]
    }
  ]
}
